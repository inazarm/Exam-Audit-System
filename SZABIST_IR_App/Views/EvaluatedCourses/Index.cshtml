@model IEnumerable<SZABIST_IR_App.Models.uspAssignedCoursesList_Result>
@{
    ViewBag.Title = "Index";
}
<div class="card-header">
    <h5>EVALUATED COURSES LIST</h5>
</div>
<div class="container">
    <div class="container">
        <div class="row d-flex">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xs-offset-0 col-sm-offset-0 col-md-offset-0 col-lg-offset-0">
                <div class="card justify-content-center">
                    <div class="card-body">
                        <div class="col-md-12 table-responsive table-wrapper-scroll-y my-custom-scrollbar">
                            <table class="table table-bordered table-hover table-sm" id="clursterHeads">
                                <thead class="thead-default">
                                    <tr class="text-uppercase">
                                        <th class="active">Course Code</th>
                                        <th>Course Name</th>
                                        <th>Instructor</th>
                                        <th>Class/Section</th>
                                        <th>Year</th>
                                        <th>Semester</th>
                                        <th>Program</th>
                                        <th>Exam Type</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @*@Html.ActionLink("  Courses Details", "AllocatedCoursesList", new { id = item.tAllocateID }, new { @class = "btn btn-outline-primary btn-sm ti-eye" })*@
                                                @*@Ajax.ActionLink(" View Courses Details", "coursesList", new { tAllocateID = item.tAllocateID },
                                                    new AjaxOptions
                                                    {
                                                        //UpdateTargetId = "courseDetails",
                                                        OnFailure = "onFailure",
                                                        OnSuccess = "onSuccess"
                                                    }, new { @class = "btn btn-outline-primary btn-sm ti-eye", @Id = "btnSelect" })*@
                                            @item.sCourse_Code
                                            </td>
                                            <td>@item.sCourse_LongDesc</td>
                                            <td>@item.Instructor</td>
                                            <td>@item.Class</td>
                                            <td>@item.Year</td>
                                            @{
                                                if (item.Semster == "1")
                                                {
                                                    <td>Spring</td>
                                                }
                                                else if (item.Semster == "2")
                                                {
                                                    <td>Summer</td>
                                                }
                                                else
                                                {
                                                    <td>Fall</td>
                                                }
                                            }
                                            <td>@item.Program_ShortDesc</td>
                                            <td>@item.ExamTypeName</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="card justify-content-center">
                    <div id="courseDetails">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery-3.6.0.min.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
